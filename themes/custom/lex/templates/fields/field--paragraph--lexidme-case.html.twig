
{% for item in items %}
    <div class="case-card">
        <img src="{{ file_url(item.content['#node'].field_photo_grid.entity.field_grid_photo[0].entity.uri.value) }}">
        <p><a href="/node/{{ item.content['#node'].nid.value }}">{{ item.content['#node'].title.value }}</a></p>
    </div>
{% endfor %}
    