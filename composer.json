{
    "name": "lfucg/lexingtonky.gov",
    "description": "The city of Lexington's website",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "path",
            "url": "upstream-configuration"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "dropzone/dropzone",
                "version": "5.7.2",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "dropzone"
                },
                "dist": {
                    "url": "https://github.com/dropzone/dropzone/archive/v5.7.2.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.7",
        "drupal/addtoany": "^1.16",
        "drupal/admin_toolbar": "^3.1",
        "drupal/autologout": "^1.3",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap_barrio": "^5.1",
        "drupal/captcha": "^1.2",
        "drupal/chosen": "^3.0",
        "drupal/ckeditor_entity_link": "^1.2",
        "drupal/ckeditor_media_embed": "^1.11",
        "drupal/components": "^2.4",
        "drupal/composer_deploy": "^1.6",
        "drupal/contact_storage": "^1.1",
        "drupal/contentimport": "^9.3",
        "drupal/core": "^9",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9",
        "drupal/ctools": "^3.7",
        "drupal/devel": "^4.1",
        "drupal/diff": "^1.x-dev@dev",
        "drupal/draggableviews": "^2.0",
        "drupal/dropzonejs": "^2.6",
        "drupal/embed": "^1.4",
        "drupal/entity": "^1.3",
        "drupal/entity_browser": "^1.10",
        "drupal/entity_embed": "^1.2",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/field_permissions": "^1.2",
        "drupal/fieldblock": "^2.0@alpha",
        "drupal/file_browser": "^1.3",
        "drupal/flood_control": "^2.2",
        "drupal/honeypot": "^2.0",
        "drupal/imce": "^2.4",
        "drupal/masquerade": "^2.x-dev@dev",
        "drupal/metatag": "^1.16",
        "drupal/pantheon_advanced_page_cache": "^2.0",
        "drupal/paragraphs": "^1.13",
        "drupal/paragraphs_inline_entity_form": "^1.x-dev@dev",
        "drupal/paragraphs_jquery_ui_accordion": "^1.4",
        "drupal/pathauto": "^1.9",
        "drupal/redirect": "^1.8",
        "drupal/search_api": "^1.23",
        "drupal/search_api_pantheon": "^8.0",
        "drupal/search_api_solr": "^4.2",
        "drupal/smtp": "^1.0",
        "drupal/taxonomy_menu": "^3.5",
        "drupal/token": "^1.10",
        "drupal/twig_tweak": "^2.4.0",
        "drupal/twig_vardumper": "^3.0",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/upgrade_status": "^3.14",
        "drupal/views_accordion": "^2.0",
        "drupal/viewsreference": "^1.7",
        "drupal/workbench_moderation": "^1.6",
        "drush/drush": "^11 || ^12",
        "grasmash/drupal-security-warning": "^1.1.1",
        "oomphinc/composer-installers-extender": "^2.0",
        "wikimedia/composer-merge-plugin": "^2.0",
        "pantheon-systems/quicksilver-pushback": "^2.1",
        "drupal/iframe": "^2.20",
        "dropzone/dropzone": "^5.7",
        "twbs/bootstrap": "<5.0",
        "drupal/recaptcha": "^3.1",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^1.9",
        "drupal/gtranslate": "1.14",
        "drupal/hotjar": "^3.1"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^9.5",
        "behat/behat": "^3.11",
        "behat/mink": "^1.10",
        "behat/mink-extension": "^2.3",
        "dmore/behat-chrome-extension": "^1.4",
        "genesis/behat-fail-aid": "^3.7",
        "jcalderonzumba/mink-phantomjs-driver": "^0.3.3",
        "mikey179/vfsstream": "^1.6",
        "drupal/coder": "^8.3",
        "drupal/drupal-extension": "^4.2",
        "drupal/drupal-driver": "^2.2",
        "drush-ops/behat-drush-endpoint": "^9.4"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "grasmash/drupal-security-warning": true,
            "oomphinc/composer-installers-extender": true,
            "wikimedia/composer-merge-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/pantheon.upstream.yml": false,
                "[project-root]/.gitattributes": false,
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/sites/example.settings.local.php": false
            }
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:bower-asset",
                "type:npm-asset",
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/private/scripts/quicksilver/{$name}/": [
                "type:quicksilver-script"
            ]
        },
        "build-env": {
            "export-configuration": "drush config-export --yes"
        },
        "merge-plugin": {
            "include": [
                "modules/custom/*/composer.json"
            ],
            "replace": false,
            "ignore-duplicates": true
        }
    },
    "autoload": {
        "classmap": [
            "upstream-configuration/scripts/ComposerScripts.php"
        ]
    },
    "scripts": {
        "copy-bootstrap": [
            "mkdir -p web/libraries/bootstrap",
            "cp -R vendor/twbs/bootstrap/dist web/libraries/bootstrap"
        ],
        "post-install-cmd": [
            "@copy-bootstrap"
        ],
        "post-update-cmd": [
            "@composer drupal:scaffold",
            "@copy-bootstrap"
        ],
        "pre-update-cmd": [
            "DrupalComposerManaged\\ComposerScripts::preUpdate"
        ],
        "upstream-require": [
            "DrupalComposerManaged\\ComposerScripts::upstreamRequire"
        ],
        "unit-test": "echo 'No unit test step defined.'",
        "lint": "find web/modules/custom web/themes/custom -name '*.php' -exec php -l {} \\;",
        "code-sniff": [
            "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
            "./vendor/bin/phpcs --standard=Drupal --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/custom",
            "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor ./web/modules/custom",
            "./vendor/bin/phpcs --standard=DrupalPractice --extensions=php,module,inc,install,test,profile,theme,info,txt,md --ignore=node_modules,bower_components,vendor ./web/themes/custom"
        ]
    },
    "scripts-descriptions": {
        "upstream-require": "Add a dependency to an upstream. See https://pantheon.io/docs/create-custom-upstream for information on creating custom upstreams."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
