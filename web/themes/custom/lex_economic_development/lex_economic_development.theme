<?php

/**
 * @file
 * Contains themeing for lex economic development.
 */

/**
 * Alter name suggestions for the blocks so subthemes will work.
 */
function lex_economic_development_theme_suggestions_block_alter(&$suggestions, $variables) {
  foreach ($suggestions as &$suggestion) {
    $suggestion = str_replace('lex_economic_development_', 'lex_', $suggestion);
  }
}
